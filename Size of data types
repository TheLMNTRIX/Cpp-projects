#include <conio.h>
#include <iostream>
using namespace std;

int main()
{
	cout << "Size of char: " << sizeof(char) << "bytes";
	cout << "\n Size of int: " << sizeof(int) << "bytes";
	cout << "\n Size of float:" << sizeof(float) << "bytes";
	cout << "\n Size of double:" << sizeof(double) << "bytes";
	return 0;
 
}
